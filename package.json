{
  "name": "jonas-thelemann",
  "version": "5.8.0",
  "scheduleVersion": "5.5.1",
  "private": true,
  "description": "The source code of `jonas-thelemann.de`.",
  "type": "module",
  "engines": {
    "node": "19"
  },
  "packageManager": "pnpm@7.27.0",
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "start": "npx http-server .output/public -p 3000",
    "generate": "nuxt generate",
    "cypress:open": "cypress open",
    "cypress:test": "cypress run --env type=actual",
    "lint": "pnpm lint:js && pnpm lint:ts && pnpm lint:style",
    "lint:js": "eslint --cache --ext .js,.ts,.vue --ignore-path .gitignore .",
    "lint:ts": "pnpm nuxt typecheck",
    "lint:style": "stylelint **/*.{vue,css} --ignore-path .gitignore",
    "lint:fix": "pnpm lint:js --fix && pnpm lint:ts --fix && pnpm lint:style --fix",
    "lint:quiet": "pnpm lint:js --quiet && pnpm lint:style --quiet",
    "lint:staged": "pnpm lint-staged",
    "prepare": "pnpm nuxt prepare && if [ \"$NODE_ENV\" != \"production\" ]; then husky install && shx rm -rf .git/hooks && shx ln -s ../.husky .git/hooks; fi",
    "test:integration:dev": "cross-env NUXT_PUBLIC_IS_TESTING=1 WAIT_ON_TIMEOUT=100000 start-server-and-test 'pnpm dev' 'http://localhost:3000' 'pnpm cypress:test'",
    "test:integration:prod": "cross-env NODE_ENV=production NUXT_PUBLIC_IS_TESTING=1 WAIT_ON_TIMEOUT=50000 start-server-and-test 'pnpm generate && pnpm start' 'http://localhost:3000' 'pnpm cypress:test'",
    "test:integration:docker:build": "docker build -t test-integration_base --build-arg UID=$(id -u) --build-arg GID=$(id -g) --target test-integration_base .",
    "test:integration:docker:run": "docker run --rm --entrypoint '' -v \"$PWD:/srv/app\" test-integration_base",
    "test:integration:docker:br": "pnpm test:integration:docker:build && pnpm test:integration:docker:run",
    "test:integration:docker:dev": "pnpm test:integration:docker:br pnpm test:integration:dev",
    "test:integration:docker:prod": "pnpm test:integration:docker:br pnpm test:integration:prod"
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.3",
    "@commitlint/config-conventional": "17.4.3",
    "@dargmuesli/nuxt-cookie-control": "5.0.0-beta.2",
    "@funken-studio/sitemap-nuxt-3": "4.0.4",
    "@intlify/eslint-plugin-vue-i18n": "2.0.0",
    "@nuxtjs/eslint-config-typescript": "12.0.0",
    "@nuxtjs/html-validator": "1.2.3",
    "@nuxtjs/i18n": "npm:@nuxtjs/i18n-edge@8.0.0-beta.9-27939896.9b0ebb4",
    "@nuxtjs/robots": "3.0.0",
    "@tailwindcss/typography": "0.5.9",
    "@types/node": "18.13.0",
    "autoprefixer": "10.4.13",
    "consola": "2.15.3",
    "cross-env": "7.0.3",
    "cypress": "12.5.1",
    "cypress-visual-regression": "2.1.1",
    "defu": "6.1.2",
    "eslint": "8.34.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-nuxt": "4.0.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-yml": "1.5.0",
    "husky": "8.0.3",
    "is-https": "4.0.0",
    "lint-staged": "13.1.2",
    "nuxt": "3.2.0",
    "postcss": "8.4.21",
    "postcss-html": "1.5.0",
    "prettier": "2.8.4",
    "prettier-plugin-tailwindcss": "0.2.2",
    "shx": "0.3.4",
    "start-server-and-test": "1.15.4",
    "stylelint": "15.1.0",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-recommended-vue": "1.4.0",
    "stylelint-config-standard": "30.0.1",
    "stylelint-no-unsupported-browser-features": "6.1.0",
    "tailwindcss": "3.2.6",
    "typescript": "4.9.5",
    "vue": "3.2.47",
    "vue-gtag": "2.0.1",
    "vue-tsc": "1.0.24"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "cosmiconfig-toml-loader",
        "webpack"
      ]
    }
  }
}
