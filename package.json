{
  "devDependencies": {
    "@commitlint/cli": "20.3.0",
    "@commitlint/config-conventional": "20.3.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "13.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "12.0.2",
    "@semantic-release/npm": "13.1.3",
    "@semantic-release/release-notes-generator": "14.1.0",
    "cross-env": "10.1.0",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "semantic-release": "25.0.2"
  },
  "engines": {
    "bun": "1.3.5"
  },
  "overrides": {
    "nuxt-og-image": "5.1.12"
  },
  "packageManager": "bun@1.3.5",
  "private": true,
  "scripts": {
    "build": "bun run --cwd src build",
    "build:node": "bun run --cwd src build:node",
    "build:static": "bun run --cwd src build:static",
    "build:static:test": "bun run --cwd src build:static:test",
    "build:test": "bun run --cwd src build:test",
    "dev": "bun run --cwd src dev",
    "generate": "bun run --cwd src generate",
    "lint": "bun -r run lint",
    "prepare": "husky",
    "start": "bun run --cwd src start",
    "start:dev": "bun run --cwd src start:dev",
    "start:node": "bun run --cwd src start:node",
    "start:node:test": "bun run --cwd src start:node:test",
    "start:static": "bun run --cwd src start:static",
    "start:static:test": "bun run --cwd src start:static:test",
    "start:test": "bun run --cwd src start:test",
    "test:e2e:docker:server:dev": "bun run --cwd tests test:e2e:docker:server:dev",
    "test:e2e:docker:server:dev:update": "bun run --cwd tests test:e2e:docker:server:dev:update",
    "test:e2e:docker:server:node": "bun run --cwd tests test:e2e:docker:server:node",
    "test:e2e:docker:server:node:update": "bun run --cwd tests test:e2e:docker:server:node:update",
    "test:e2e:docker:server:static": "bun run --cwd tests test:e2e:docker:server:static",
    "test:e2e:docker:server:static:update": "bun run --cwd tests test:e2e:docker:server:static:update",
    "test:e2e:ui": "bun run --cwd tests test:e2e:ui"
  },
  "trustedDependencies": ["sharp", "src"],
  "type": "module",
  "workspaces": ["src", "tests"]
}
