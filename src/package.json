{
  "description": "The source code of `jonas-thelemann.de`.",
  "devDependencies": {
    "@dargmuesli/nuxt-cookie-control": "9.1.12",
    "@dargmuesli/nuxt-vio": "20.1.5",
    "@nuxt/scripts": "0.13.2",
    "@nuxtjs/turnstile": "1.1.1",
    "@types/nodemailer": "7.0.4",
    "@vuelidate/core": "2.0.3",
    "@vuelidate/validators": "2.0.4",
    "consola": "3.4.2",
    "defu": "6.1.4",
    "eslint": "9.39.2",
    "h3": "1.15.4",
    "nodemailer": "7.0.12",
    "nuxt": "4.2.2",
    "pinia": "3.0.4",
    "prettier-plugin-tailwindcss": "0.7.2",
    "serve": "14.2.5",
    "stylelint": "16.26.1",
    "stylelint-config-recommended-vue": "1.6.1",
    "stylelint-config-standard": "39.0.1",
    "stylelint-no-unsupported-browser-features": "8.0.5",
    "tailwindcss": "4.1.18",
    "ufo": "1.6.2",
    "vite": "7.3.1",
    "vue": "3.5.26",
    "vue-router": "4.6.4",
    "vue-tsc": "3.2.2"
  },
  "engines": {
    "bun": "1.3.5"
  },
  "name": "src",
  "private": true,
  "scripts": {
    "build": "bun run build:static",
    "build:cloudflare_pages": "NITRO_PRESET=cloudflare_pages nuxt build",
    "build:node": "nuxt build",
    "build:static": "nuxt generate",
    "build:static:test": "cross-env NUXT_PUBLIC_VIO_IS_TESTING=true SITE_URL=https://localhost:3002 bun run build:static",
    "build:test": "bun run build:static:test",
    "dev": "bun run start:dev",
    "generate": "bun run build:static",
    "lint": "bun run lint:js && bun run lint:ts && bun run lint:style",
    "lint:fix": "bun run lint:js --fix && bun run lint:ts --fix && bun run lint:style --fix",
    "lint:js": "eslint --cache",
    "lint:staged": "lint-staged",
    "lint:style": "stylelint --cache \"**/*.{vue,css}\" --ignore-path .gitignore",
    "lint:ts": "nuxt typecheck",
    "prepare": "nuxt prepare && ./.config/certificates/mkcert.sh",
    "preview": "nuxt preview",
    "start": "bun run start:static",
    "start:dev": "nuxt dev",
    "start:node": "node .output/server/index.mjs",
    "start:node:test": "PORT=3001 bun run start:node",
    "start:static": "serve .output/public --ssl-cert ./.config/certificates/ssl.crt --ssl-key ./.config/certificates/ssl.key",
    "start:static:test": "PORT=3002 bun run start:static",
    "start:test": "bun run start:static:test"
  },
  "type": "module",
  "version": "6.1.3"
}
